                <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
                >true</rdf:first>
              </rdf:List>
            </rdf:rest>
            <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
            >false</rdf:first>
          </rdf:List>
        </owl:oneOf>
      </rdfs:Datatype>
    </rdfs:range>
