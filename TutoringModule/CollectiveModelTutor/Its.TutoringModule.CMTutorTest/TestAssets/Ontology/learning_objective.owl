<?xml version="1.0"?>
<rdf:RDF
    xmlns:protege="http://protege.stanford.edu/plugins/owl/protege#"
    xmlns:xsp="http://www.owl-ontologies.com/2005/08/07/xsp.owl#"
    xmlns="http://www.owl-ontologies.com/learning_objective.owl#"
    xmlns:p1="http://www.owl-ontologies.com/assert.owl#"
    xmlns:Learning_Objective="http://www.owl-ontologies.com/learning_objective.owl#"
    xmlns:owl2xml="http://www.w3.org/2006/12/owl2-xml#"
    xmlns:Student_Profile="http://www.owl-ontologies.com/student_profile.owl#"
    xmlns:swrlb="http://www.w3.org/2003/11/swrlb#"
    xmlns:knowledge_object="http://www.owl-ontologies.com/knowledge_object.owl#"
    xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
    xmlns:student_profile="http://www.owl-ontologies.com/student_profile.owl#"
    xmlns:owl="http://www.w3.org/2002/07/owl#"
    xmlns:Knowledge_Object="http://www.owl-ontologies.com/knowledge_object.owl#"
    xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
    xmlns:swrl="http://www.w3.org/2003/11/swrl#"
    xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
    xmlns:unnamed="http://www.owl-ontologies.com/unnamed.owl#"
  xml:base="http://www.owl-ontologies.com/learning_objective.owl">
  <owl:Ontology rdf:about="">
    <owl:imports rdf:resource="http://www.owl-ontologies.com/student_profile.owl"/>
    <owl:versionIRI rdf:resource=""/>
    <owl:imports rdf:resource="http://www.owl-ontologies.com/knowledge_object.owl"/>
  </owl:Ontology>
  <owl:Class rdf:about="http://www.owl-ontologies.com/knowledge_object.owl#Knowledge_Object"/>
  <owl:Class rdf:ID="Fear_Control">
    <rdfs:subClassOf>
      <owl:Class rdf:ID="Emotional_Ability"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:ID="Self_Control">
    <rdfs:subClassOf>
      <owl:Class rdf:about="#Emotional_Ability"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:ID="Requires_Plan_To_Achieve">
    <rdfs:subClassOf>
      <owl:Class rdf:ID="Knows_That"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:ID="Self_Regulation">
    <rdfs:subClassOf>
      <owl:Class rdf:about="#Emotional_Ability"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:ID="Knows_That_Action_In_Plan_Post">
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:cardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger"
        >1</owl:cardinality>
        <owl:onProperty>
          <owl:ObjectProperty rdf:ID="requiresKnowledgeObject"/>
        </owl:onProperty>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Class rdf:about="#Knows_That"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:ID="Is_Able_To_Choose">
    <rdfs:subClassOf>
      <owl:Class rdf:ID="Objective_Application"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:ID="What_Is_For">
    <rdfs:subClassOf>
      <owl:Class rdf:ID="Knows_What_Is"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:ID="Knows_Where_Is">
    <rdfs:subClassOf>
      <owl:Class rdf:ID="Objective_Knowledge"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:ID="Objective_Basis_Fundamental_Movement">
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >Movimiento simple; llevar a cabo acciones sencillas: saltar, cambiar de dirección, pasear,</rdfs:comment>
    <rdfs:subClassOf>
      <owl:Class rdf:ID="Psychomotor_Level_Objective"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:ID="Self_Known">
    <rdfs:subClassOf>
      <owl:Class rdf:about="#Emotional_Ability"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:about="http://www.owl-ontologies.com/student_profile.owl#Student_Profile"/>
  <owl:Class rdf:ID="Objective_Interest">
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >Responder, mostrar interés</rdfs:comment>
    <rdfs:subClassOf>
      <owl:Class rdf:ID="Objective_Affective_Level"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:ID="Objective_Attention">
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >Recibir, atender</rdfs:comment>
    <rdfs:subClassOf>
      <owl:Class rdf:about="#Objective_Affective_Level"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:ID="Objective_Communication_Non_Discursive">
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >Expresar o comunicar sentimientos o algo a través del movimiento y acciones.</rdfs:comment>
    <rdfs:subClassOf>
      <owl:Class rdf:about="#Psychomotor_Level_Objective"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:about="#Objective_Application">
    <rdfs:subClassOf>
      <owl:Class rdf:ID="Cognitive_Level_Objective"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:about="http://www.w3.org/2002/07/owl#Thing"/>
  <owl:Class rdf:ID="Learning_Objective"/>
  <owl:Class rdf:ID="Knows_That_Arg_Order_Next_Action_Is">
    <rdfs:subClassOf>
      <owl:Class rdf:about="#Knows_That"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:ID="Class_2">
    <rdfs:subClassOf>
      <owl:Class rdf:ID="Is_Able_To_Design"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:ID="Stress_Control">
    <rdfs:subClassOf>
      <owl:Class rdf:about="#Emotional_Ability"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:about="#Objective_Knowledge">
    <rdfs:subClassOf>
      <owl:Class rdf:about="#Cognitive_Level_Objective"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:ID="Knows_Recognize">
    <rdfs:subClassOf rdf:resource="#Objective_Knowledge"/>
  </owl:Class>
  <owl:Class rdf:about="#Cognitive_Level_Objective">
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >Taxonomía de Bloom</rdfs:comment>
    <rdfs:subClassOf>
      <owl:Class rdf:ID="Specific_Objective"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:ID="Empathy">
    <rdfs:subClassOf>
      <owl:Class rdf:about="#Emotional_Ability"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:ID="Is_Able_To_Obtain">
    <rdfs:subClassOf rdf:resource="#Objective_Application"/>
  </owl:Class>
  <owl:Class rdf:ID="Knows_That_Next_Action_Is">
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:cardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger"
        >1</owl:cardinality>
        <owl:onProperty>
          <owl:ObjectProperty rdf:about="#requiresKnowledgeObject"/>
        </owl:onProperty>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Class rdf:about="#Knows_That"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:ID="Objective_Analysis">
    <rdfs:subClassOf rdf:resource="#Cognitive_Level_Objective"/>
  </owl:Class>
  <owl:Class rdf:ID="Curiosity">
    <rdfs:subClassOf>
      <owl:Class rdf:about="#Emotional_Ability"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:ID="Objective_Skilled_Movement">
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >Operaciones complejas: jugar, nadar, conducir, construir, tocar un instrumento musical,...</rdfs:comment>
    <rdfs:subClassOf>
      <owl:Class rdf:about="#Psychomotor_Level_Objective"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:about="#Specific_Objective">
    <rdfs:subClassOf rdf:resource="#Learning_Objective"/>
  </owl:Class>
  <owl:Class rdf:about="http://www.owl-ontologies.com/knowledge_object.owl#Property"/>
  <owl:Class rdf:ID="Is_Able_To_Build">
    <rdfs:subClassOf rdf:resource="#Objective_Application"/>
  </owl:Class>
  <owl:Class rdf:ID="Objective_Perceptual_Ability">
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >uso de una capacidad en respuesta a distintas percepciones sensoriales. Explorar, distinguir, manipular...</rdfs:comment>
    <rdfs:subClassOf>
      <owl:Class rdf:about="#Psychomotor_Level_Objective"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:ID="Objective_Reflex">
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >respuesta involuntaria física. Responder o reaccionar a algo</rdfs:comment>
    <rdfs:subClassOf>
      <owl:Class rdf:about="#Psychomotor_Level_Objective"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:ID="Trust">
    <rdfs:subClassOf>
      <owl:Class rdf:about="#Emotional_Ability"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:ID="Is_Able_To_Analize">
    <rdfs:subClassOf rdf:resource="#Objective_Analysis"/>
  </owl:Class>
  <owl:Class rdf:ID="Knows_That_Action_Object_In_Plan_Post">
    <rdfs:subClassOf>
      <owl:Class rdf:about="#Knows_That"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:about="#Objective_Affective_Level">
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >Taxonomía de Krathwohl</rdfs:comment>
    <rdfs:subClassOf rdf:resource="#Specific_Objective"/>
  </owl:Class>
  <owl:Class rdf:ID="Objective_Attitude">
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >El estudiante valora lo que se enseña. Diferencia, demuestra, explica, ....</rdfs:comment>
    <rdfs:subClassOf rdf:resource="#Objective_Affective_Level"/>
  </owl:Class>
  <owl:Class rdf:ID="Objective_Comprehension">
    <rdfs:subClassOf rdf:resource="#Cognitive_Level_Objective"/>
  </owl:Class>
  <owl:Class rdf:ID="Is_Able_To_Apply">
    <rdfs:subClassOf rdf:resource="#Objective_Application"/>
  </owl:Class>
  <owl:Class rdf:ID="Objective_Synthesis">
    <rdfs:subClassOf rdf:resource="#Cognitive_Level_Objective"/>
  </owl:Class>
  <owl:Class rdf:ID="Objective_Physical_Ability">
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >desarrollar resistencia o fuerza, agilidad, control. resistir, parar, mejorar, moverse rápidamente.</rdfs:comment>
    <rdfs:subClassOf>
      <owl:Class rdf:about="#Psychomotor_Level_Objective"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:about="#Knows_What_Is">
    <rdfs:subClassOf rdf:resource="#Objective_Knowledge"/>
  </owl:Class>
  <owl:Class rdf:about="#Knows_That">
    <rdfs:subClassOf rdf:resource="#Objective_Knowledge"/>
  </owl:Class>
  <owl:Class rdf:ID="Objective_Evaluation">
    <rdfs:subClassOf rdf:resource="#Cognitive_Level_Objective"/>
  </owl:Class>
  <owl:Class rdf:about="#Psychomotor_Level_Objective">
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >Harrow</rdfs:comment>
    <rdfs:subClassOf rdf:resource="#Specific_Objective"/>
  </owl:Class>
  <owl:Class rdf:ID="Didactic_Objective">
    <rdfs:subClassOf rdf:resource="#Learning_Objective"/>
  </owl:Class>
  <owl:Class rdf:about="#Is_Able_To_Design">
    <rdfs:subClassOf rdf:resource="#Objective_Synthesis"/>
  </owl:Class>
  <owl:Class rdf:about="#Emotional_Ability">
    <rdfs:subClassOf rdf:resource="#Objective_Affective_Level"/>
  </owl:Class>
  <owl:ObjectProperty rdf:ID="consistOfObjectives">
    <rdfs:range rdf:resource="#Learning_Objective"/>
    <rdfs:domain rdf:resource="#Didactic_Objective"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:ID="describesFollowUpOf">
    <rdfs:range rdf:resource="http://www.owl-ontologies.com/student_profile.owl#Student_Profile"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="#requiresKnowledgeObject">
    <rdfs:domain rdf:resource="#Learning_Objective"/>
    <rdfs:range rdf:resource="http://www.owl-ontologies.com/knowledge_object.owl#Knowledge_Object"/>
  </owl:ObjectProperty>
  <owl:DatatypeProperty rdf:ID="requiredByActivities">
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    <rdfs:domain rdf:resource="#Learning_Objective"/>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >identificadores de Actividades correspondientes. En realidad, object property -&gt; ontología asociada</rdfs:comment>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID="automationDegree">
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    <rdfs:domain rdf:resource="#Objective_Skilled_Movement"/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID="isAchievedFromActivities">
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    <rdfs:domain rdf:resource="#Learning_Objective"/>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >identificadores de Actividades correspondientes. En realidad, object property de actividad</rdfs:comment>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID="isAchievedFromPhases">
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    <rdfs:domain rdf:resource="#Didactic_Objective"/>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >ident. de las Fases correspondientes. object property de ontología correspondiente.</rdfs:comment>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID="requiredByPhases">
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    <rdfs:domain rdf:resource="#Didactic_Objective"/>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >id. de Fases correspondientes. object property de ontología correspondiente.</rdfs:comment>
  </owl:DatatypeProperty>
  <owl:FunctionalProperty rdf:ID="objectiveDescriptor">
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    <rdfs:domain rdf:resource="#Learning_Objective"/>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >Conocer el concepto</rdfs:comment>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
  </owl:FunctionalProperty>
  <owl:FunctionalProperty rdf:ID="propName">
    <rdfs:domain rdf:resource="http://www.owl-ontologies.com/knowledge_object.owl#Property"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
  </owl:FunctionalProperty>
  <owl:FunctionalProperty rdf:ID="applicationQuality">
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#float"/>
    <rdfs:domain rdf:resource="#Is_Able_To_Apply"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
  </owl:FunctionalProperty>
  <owl:FunctionalProperty rdf:ID="levelReliability">
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#int"/>
    <rdfs:domain rdf:resource="#Specific_Objective"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
  </owl:FunctionalProperty>
  <owl:FunctionalProperty rdf:ID="identifierObjective">
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    <rdfs:domain rdf:resource="#Learning_Objective"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
  </owl:FunctionalProperty>
</rdf:RDF>

